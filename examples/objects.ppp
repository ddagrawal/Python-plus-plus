#include "ppp.ppp"
exec(open("ppp.ppp").read())

class Foo:
	members(""" ",
		a,
		xyz,
		b,
		foo,
	" """)
	
	def m0(margs):
		self.a = self.a + 1;
	end
	
	def m1(margs):
		print(self.a);
	end
end

def run(args):
	print("Hello World!");
	
	var
	foo = make(Foo, 8, 1, 0, 7);
	
	foo.o.m1();
	foo.o.m0();
	foo.o.m0();
	foo.o.m1();
end

end_program()
