#include "ppp.ppp"
exec(open("ppp.ppp").read())

class Bar:
	members(""" ",
		a, b, c,
	" """)
	def meth(args):
		print(self.a);
	end
end_class

def run(args):
	print("Hello World!");
	
	var
	data = make(Bar, 12, 5.2, str("test"));
	data.meth();
	print(data.self.b);
	print(data.self.c);
	
end

